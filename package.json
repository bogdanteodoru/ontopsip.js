{
	"name": "ontopsip.js",
	"version": "0.2.6",
	"description": "Small library on top sip.js with some new features",
	"license": "MIT",
	"repository": "https://github.com/bogdanteodoru/ontopsip.js",
	"author": {
		"name": "Bogdan Teodoru",
		"email": "me@bogdanteodoru.com",
		"url": "https://www.bogdanteodoru.com"
	},
	"main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
  "type": "module",
	"engines": {
		"node": ">=9"
	},
	"nyc": {
		"require": [
			"ts-node/register"
		],
		"extends": "@istanbuljs/nyc-config-typescript",
		"all": true,
		"check-coverage": true,
		"include": [
			"src/**/*.ts"
		]
	},
	"scripts": {
		"clean": "rm -fR dist/* build/*",
		"build": "npm run clean && rollup -c",
		"build-watch": "npm run clean && rollup -cw",
		"typecheck": "tsc",
		"lint": "eslint \"src/**/*.ts\"",
		"prepare": "npm run lint && npm run build",
		"docs": "typedoc --out docs src",
		"run-e2e-tests-in-docker": "docker-compose up --exit-code-from"
	},
	"files": [
		"src",
		"dist",
		"README.md"
	],
	"keywords": [
		"sip.js",
		"extended"
	],
	"dependencies": {
		"p-retry": "^6.1.0",
		"p-timeout": "^6.1.2",
		"sip.js": "0.21.2"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"@types/events": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^3.1.0",
		"@typescript-eslint/parser": "^2.7.0",
		"base": "^0.13.2",
		"dotenv": "^8.2.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"fast-check": "^1.17.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^14.1.1",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"rollup": "^1.23.1",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-typescript2": "^0.24.3",
		"ts-node": "^8.4.1",
		"typedoc": "^0.15.0",
		"typescript": "^3.6.3"
	}
}
